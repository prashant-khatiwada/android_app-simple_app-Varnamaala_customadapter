<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.momobites.www.speaknepali">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Basic.Letters"
            android:label="@string/category_letters" />
        <activity
            android:name=".Basic.Numbers"
            android:label="@string/category_numbers" />
        <activity
            android:name=".Basic.Family"
            android:label="@string/category_family" />
        <activity
            android:name=".Basic.Colors"
            android:label="@string/category_colors" />
        <activity
            android:name=".Basic.Time"
            android:label="@string/category_time" />
        <activity
            android:name=".Basic.Phrases"
            android:label="@string/category_phrases" />


    </application>

</manifest>